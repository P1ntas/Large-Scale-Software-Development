            "editorMode": "code",
            "format": "table",
            "rawQuery": true,
            "rawSql": "SELECT\n    f.name AS factory_name,\n    sm.name AS system_name,\n    s.status AS system_status,\n    em.name AS expansion_name,\n    e.status AS expansion_status,\n    senm.name AS sensor_name,\n    se.status AS sensor_status\nFROM\n    factory f\nJOIN\n    system s ON f.id = s.factory_id\nJOIN\n    system_model sm ON s.system_model_id = sm.id\nJOIN\n    expansion e ON s.id = e.system_id\nJOIN\n    expansion_model em ON e.expansion_model_id = em.id\nJOIN\n    sensor se ON e.id = se.expansion_id\nJOIN\n    sensor_model senm ON se.sensor_model_id = senm.id;\n",
            "refId": "A",
